{
  "name": "affilate-crm-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api:dev": "wrangler dev --minify src/server/index.ts",
    "api:deploy": "wrangler deploy --minify --name adscrush-api --keep-vars src/server/index.ts",
    "xata:upload": "xata schema upload schema.json",
    "xata:dump": "xata schema dump -f schema.json",
    "xata:pull": "xata pull main && bun run db:pull",
    "xata:push": "xata push main",
    "xata:one-click": "xata pull main && bun run pg:pull",
    "pg:push": "drizzle-kit push --config src/db/xata/drizzle.config.ts",
    "pg:pull": "drizzle-kit pull --config src/db/xata/drizzle.config.ts",
    "turso:push": "drizzle-kit push --config src/db/turso/drizzle.config.ts",
    "turso:pull": "drizzle-kit pull --config src/db/turso/drizzle.config.ts",
    "turso:studio": "drizzle-kit studio --config src/db/turso/drizzle.config.ts"
  },
  "dependencies": {
